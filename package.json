{
  "name": "mash-3d-configurator",
  "private": true,
  "version": "0.2.7",
  "description": "mashroom 3d configurator",
  "author": "medab",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "build:lib": "vite build && npm run build:css",
    "build:lib:watch": "concurrently \"vite build --watch\" \"npm run build:css:watch\"",
    "build:css": "postcss lib/styles/tailwind.css -o dist/styles.css",
    "dev:test": "cd test-environment && npm run dev",
    "build:test": "cd test-environment && npm run build",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --write --ignore-unknown",
    "release": "standard-version",
    "publish": "node publish.js"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "mash-3d-configurator": "file:",
    "three": "^0.175.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/builder-vite": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@tailwindcss/postcss": "^4.0.8",
    "@tailwindcss/vite": "^4.0.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^3.0.5",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.11.2",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "standard-version": "^9.5.0",
    "storybook": "^8.5.3",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5"
  }
}
